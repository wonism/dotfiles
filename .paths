#!/usr/bin/env bash

export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:

# Add `~/bin` to the `$PATH`
export PATH="$HOME/bin:$PATH";

export PATH=$PATH:~/.pub-cache/bin

# Go lang
export GOPATH=$HOME/go
export PATH=$PATH:$GOPATH/bin

# Mongo DB
export MONGO_PATH='/usr/local/mongodb-3.2.4'
export PATH=$PATH:$MONGO_PATH/bin

# Cargo
export PATH="$PATH:$HOME/.cargo/bin"

# deno
export PATH="$PATH:$HOME/.deno/bin"

# elixir
export PATH="$PATH:/usr/local/Cellar/elixir/1.10.4"

# The next line updates PATH for the Google Cloud SDK.
if [ -f "$HOME/google-cloud-sdk/path.bash.inc" ]; then source "$HOME/google-cloud-sdk/path.bash.inc"; fi

# The next line enables shell command completion for gcloud.
if [ -f "$HOME/google-cloud-sdk/completion.bash.inc" ]; then source "$HOME/google-cloud-sdk/completion.bash.inc"; fi

export DYLD_LIBRARY_PATH=$DYLD_LIBRARY_PATH:/usr/local/opt/python/Frameworks/Python.framework/Versions/3.7/Python

export PATH="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin:$PATH"

export PATH="/usr/local/opt/ruby/bin:$PATH"
export PATH="/usr/local/Cellar/parallel/20201022:$PATH"

# Hub
#eval "$(hub alias -s)"

# Node.js
#export NVM_DIR="$HOME/.nvm"
#[ -s "/usr/local/opt/nvm/nvm.sh" ] && . "/usr/local/opt/nvm/nvm.sh"  # This loads nvm
#[ -s "/usr/local/opt/nvm/etc/bash_completion" ] && . "/usr/local/opt/nvm/etc/bash_completion"  # This loads nvm bash_completion

# Go
# - bin
#export PATH=$PATH:$HOME/go/bin
export PATH=$PATH:/usr/local/go/bin
